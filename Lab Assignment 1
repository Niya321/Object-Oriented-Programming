 import java.util.Scanner;

public class tempreature {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("********************************")
		System.out.print("Please enter tempreature in Celsuis: ");
		int tempcel= input.nextInt();
		System.out.println("********************************")
		int tempfar=((tempcel*9)/5)+32;
		System.out.println(" Tempreture in Fahrenheit is: " + tempfar); 
	}
}
